{
  "private": true,
  "version": "0.0.1",
  "description": "Covailnt App",
  "name": "covailnt",
  "main": "",
  "scripts": {
    "start": "npm run clean && npm run dev",
    "clean": "rimraf dist/*",
    "dev": "npm run env:dev && webpack-dev-server --colors --progress",
    "env:dev": "cross-env NODE_ENV=development",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "deploy":
      "npm run env:dev && npm run clean && npm run dist && firebase deploy",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format":
      "prettier-eslint --write \"src/**/**/*.js\" \"src/**/**/**/*.js\" \"src/**/**/**/**/*.js\" \"_src/**/**/*.js\" \"_src/**/**/**/*.js\" \"_src/**/**/**/**/*.js\"",
    "lint": "eslint ./src",
    "lint:css": "stylelint './src/**/**/*.js'",
    "lint-staged": "lint-staged",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "sort": "import-sort --overwrite \"src\"",
    "storybook": "start-storybook -p 9001 -c private/storybook"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier-eslint --write --config .prettierrc --eslint-config-path .eslintrc ",
      "git add"
    ]
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "repository": "https://github.com/covailnt/app",
  "author": "Ryan Garant",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "customized-progress-webpack-plugin": "0.0.3",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "import-sort-cli": "^3.3.0",
    "import-sort-parser-babylon": "^3.2.0",
    "import-sort-style-module": "^3.0.0",
    "lint-staged": "^4.3.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.4",
    "prettier-eslint-cli": "^4.4.0",
    "react-hot-loader": "3.1.1",
    "react-storybook-decorator-background": "^1.0.5",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.19.0",
    "styled-theming": "^2.2.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-blocks": "^1.0.0-rc.2",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "aphrodite": "^1.2.5",
    "firebase": "^4.6.0",
    "generate-password": "^1.3.0",
    "palx": "^1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.1",
    "react-modal": "^3.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "rebass": "^1.0.4",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-watch": "^1.1.1",
    "regenerator-runtime": "^0.11.0",
    "styled-components": "^2.2.3",
    "styled-media-query": "^2.0.1",
    "styled-shortcut-components": "^1.0.0",
    "styled-shortcuts": "^2.1.2",
    "styled-system": "^1.0.8",
    "styled-tools": "^0.2.2",
    "underscore": "^1.8.3"
  }
}
