{
  "private": true,
  "version": "0.0.1",
  "description": "Covailnt App",
  "name": "covailnt",
  "main": "",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist/*",
    "dev": "npm run env:dev && webpack-dev-server",
    "env:dev": "cross-env NODE_ENV=development",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "deploy": "npm run env:dev && npm run clean && npm run dist && firebase deploy",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format": "prettier-eslint --write \"src/**/**/*.js\" \"src/**/**/**/*.js\" \"src/**/**/**/**/*.js\" \"_src/**/**/*.js\" \"_src/**/**/**/*.js\" \"_src/**/**/**/**/*.js\"",
    "lint": "eslint ./src",
    "lint:css": "stylelint './src/**/**/*.js'",
    "lint-staged": "lint-staged",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "storybook": "start-storybook -p 9001 -c private/storybook"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/covailnt/app",
  "author": "Ryan Garant",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.9.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.4",
    "prettier-eslint-cli": "^4.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-storybook-decorator-background": "^1.0.5",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.4.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.13.0",
    "styled-theming": "^2.2.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^3.8.1",
    "webpack-blocks": "^1.0.0-rc.2"
  },
  "dependencies": {
    "aphrodite": "^1.2.1",
    "firebase": "^4.1.3",
    "generate-password": "^1.3.0",
    "palx": "^1.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-dropzone": "^3.13.3",
    "react-modal": "^2.0.6",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "rebass": "^1.0.4",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.4",
    "redux-watch": "^1.1.1",
    "regenerator-runtime": "^0.11.0",
    "styled-components": "^2.2.2",
    "underscore": "^1.8.3",
    "webpack-dev-server": "^2.9.3"
  }
}
