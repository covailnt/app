{
  "private": true,
  "version": "0.0.1",
  "description": "Covailnt App",
  "name": "covailnt",
  "scripts": {
    "dev": "cd \"src/app\" && yarn dev",
    "prebuild": "rimraf \"dist/functions\"",
    "build": "yarn build-functions && yarn build-app",
    "postbuild": "cd \"dist/functions\" && yarn",
    "build-app": "cd \"src/app\" && yarn build",
    "build-functions":
      "babel \"src/functions\" --out-dir \"dist/functions\" --copy-files --ignore \"node_modules\"",
    "predeploy": "yarn build",
    "deploy": "firebase deploy",
    "postinstall": "yarn install-app && yarn install-functions",
    "install-app": "cd \"src/app\" && yarn",
    "install-functions": "cd \"src/functions\" && yarn",
    "preserve": "yarn build",
    "serve": "firebase serve --only functions,hosting",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format":
      "prettier-eslint --write \"src/**/**/*.js\" \"src/**/**/**/*.js\" \"src/**/**/**/**/*.js\" \"_src/**/**/*.js\" \"_src/**/**/**/*.js\" \"_src/**/**/**/**/*.js\"",
    "icons": "node src/utils/scripts/parseIcons",
    "lint": "eslint ./src",
    "lint:css": "stylelint './src/**/**/*.js'",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "sort": "import-sort --overwrite \"src\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier-eslint --write --config .prettierrc --eslint-config-path .eslintrc ",
      "git add"
    ]
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "repository": "https://github.com/covailnt/app",
  "author": "Ryan Garant",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.15",
    "@storybook/react": "^3.2.15",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "camelcase": "^4.1.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "import-sort-cli": "^3.3.0",
    "import-sort-parser-babylon": "^3.2.0",
    "import-sort-style-module": "^3.0.0",
    "lint-staged": "^4.3.0",
    "next": "^4.1.4",
    "prettier": "^1.7.4",
    "prettier-eslint-cli": "^4.4.0",
    "react-storybook-decorator-background": "^1.0.5",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "styled-reset": "^0.1.3",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "svgi": "^1.1.0"
  },
  "dependencies": {
    "firebase": "^4.6.0",
    "generate-password": "^1.3.0",
    "lodash": "^4.17.4",
    "palx": "^1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.1",
    "react-modal": "^3.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-watch": "^1.1.1",
    "regenerator-runtime": "^0.11.0",
    "styled-components": "^2.2.3",
    "styled-media-query": "^2.0.1",
    "styled-system": "^1.0.8",
    "styled-tools": "^0.2.2"
  }
}
