{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 3 versions"]
        },
        "debug": true
      }
    ],
    "react"
  ],
  "plugins": ["transform-export-extensions", "transform-regenerator"],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
